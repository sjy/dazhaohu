{
    "env": {
        "commonjs": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "useBuiltIns": false,
                        "modules": "commonjs"
                    }
                ]
            ]
        },
        "es": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "useBuiltIns": false,
                        "modules": false
                    }
                ]
            ]
        },
        "umd": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "useBuiltIns": false,
                        "modules": "umd"
                    }
                ]
            ],
            "plugins": [
                ["@babel/plugin-transform-modules-umd", {
                    "exactGlobals": true
                }]
            ],
            "moduleId": "dzh"
        }
    },
    "plugins": [
        "babel-plugin-add-module-exports",
        ["babel-plugin-inline-json-import", {}]
    ]

}